<h1>Prefs#index</h1>
<p>Find me in app/views/prefs/index.html.erb</p>
<div class="col-md-10 col-md-offset-1">
  <div class="csv_holder">
    <%= form_tag import_prefs_path, multipart: true do %>
    <%= file_field_tag :file %>
  </div>
  <div>
    <%= submit_tag "都道府県をインポート" %>
  </div>
  <% end %>



<div class="col-md-10 col-md-offset-1">
  <% @prefs.each do |pref| %>
    <%= link_to pref, method: :get %>
  <% end %>
</div>
